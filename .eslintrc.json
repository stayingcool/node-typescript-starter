{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier",
        "unused-imports"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "unused-imports/no-unused-imports": "error"
    }
}